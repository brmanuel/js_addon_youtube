{

  "description": "blocking certain songs on youtube autoplay",
  "manifest_version": 2,
  "name": "youtubeBlocker",
  "version": "1.0",

  "permissions": [
      "webRequest",
      "webRequestBlocking",
      "cross-domain-content": ["https://www.youtube.com"]
  ],
 
  "background": {
    "scripts": ["yt.js"]
  }

}
